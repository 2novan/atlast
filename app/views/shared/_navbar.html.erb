<div id="navbar">
  <%= link_to "/" do %>
    <%= image_tag("logo.svg", :alt => "logo", id: "logo")%>
  <% end %>

  <%= form_tag search_page_path, method: :get, class: "search-bar" do %>
    <%= text_field_tag :search, params[:search], type: "search", placeholder: "Find an artist", required: "", novalidate: "novalidate"%>
    <%= button_tag "", class: "search-btn", type: "submit"%>
  <% end %>
</div>
