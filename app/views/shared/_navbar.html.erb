<div id="navbar">
  <%= image_tag("logo.svg", :alt => "logo") %>

  <%= form_tag search_page_path, method: :get, class: "search-bar" do %>
    <%= text_field_tag :search, params[:search], type: "search", placeholder: "Find an artist", required: "required"%>
    <%= submit_tag "Search", class: "search-btn", type: "submit"%>
  <% end %>
</div>
